{
  "name": "master",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "add-all": "pkg=$pkg dep=$dep npm run add:es",
    "add:es": "cd ./es/ && npm i $pkg -$dep",
    "install-all": "npm run install:config && npm run install:header && npm run install:slider && npm run install:cloudServer && npm run install:es && npm run install:parcelUtils",
    "install:config": "npm i",
    "install:header": "cd ./header/ && npm i",
    "install:slider": "cd ./slider/ && npm i",
    "install:cloudServer": "cd ./cloudServer/ && npm i",
    "install:es": "cd ./es/ && npm i",
    "install:parcelUtils": "cd ./parcelUtils/ && npm i",
    "start": "concurrently --kill-others --kill-others-on-fail --prefix name --names \"config,deps,header,slider,parcelUtils,es,cloudServer\" \"npm run start:config\" \"npm run start:deps\" \"npm run start:header\" \"npm run start:slider\" \"npm run start:parcelUtils\" \" npm run start:es\" \"npm run start:cloudServer\" ",
    "start:deps": "webpack-dev-server --config ./webpack.config.deps.dev.js",
    "start:header": "cd ./header/ && webpack-dev-server --config ./webpack.config.dev.js",
    "start:slider": "cd ./slider/ && webpack-dev-server --config ./webpack.config.dev.js ",
    "start:parcelUtils": "cd ./parcelUtils/ && webpack-dev-server --config ./webpack.config.dev.js",
    "start:es": "cd ./es/ && webpack-dev-server --config ./webpack.config.dev.js",
    "start:cloudServer": "cd ./cloudServer/ && webpack-dev-server --config ./webpack.config.dev.js",
    "start:config": "webpack-dev-server --config ./webpack.config.dev.js",
    "build:config": "webpack --config webpack.config.js",
    "build:deps": "webpack --config webpack.config.deps.js",
    "postinstall": "node postinstall.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "element-ui": "^2.7.2",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "single-spa": "4.0.1",
    "sofe": "2.1.4",
    "systemjs": "0.20.19"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-stage-0": "^7.0.0",
    "autoprefixer": "^9.5.0",
    "babel-loader": "8.0.0",
    "clean-webpack-plugin": "0.1.19",
    "concurrently": "4.0.1",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "1.0.0",
    "dotenv-defaults": "^1.0.2",
    "dotenv-webpack": "^1.7.0",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-notifier": "^5.4.0",
    "style-loader": "0.23.0",
    "url-loader": "^1.1.2",
    "webpack": "4.17.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "^3.1.14"
  }
}

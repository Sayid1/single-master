{
  "name": "master",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "concurrently --kill-others --kill-others-on-fail --prefix name --names \"config,deps,header,slider,app3\" \"yarn start:config\" \"yarn start:deps\" \"yarn start:header\" \"yarn start:slider\" \"yarn start:app3\"",
    "start:config": "webpack-dev-server --config ./webpack.config.dev.js --port 7001",
    "start:deps": "webpack-dev-server --config ./webpack.config.deps.dev.js --port 7002",
    "start:header": "cd ./header && webpack-dev-server --config ./webpack.config.dev.js --port 7003",
    "start:slider": "cd ./slider && webpack-dev-server --config ./webpack.config.dev.js --port 7004",
    "start:app3": "cd ./app3 && webpack-dev-server --config ./webpack.config.js --port 7005",
    "build": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "single-spa": "4.0.1",
    "sofe": "2.1.4",
    "systemjs": "0.20.19",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.0",
    "clean-webpack-plugin": "0.1.19",
    "concurrently": "4.0.1",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "1.0.0",
    "style-loader": "0.23.0",
    "webpack": "4.17.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "^3.1.14"
  }
}

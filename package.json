{
  "name": "master",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "install-all": "npm run install:config && npm run install:header && npm run install:slider && npm run install:app3",
    "install:config": "npm i",
    "install:header": "cd ./header/ && npm i",
    "install:slider": "cd ./slider/ && npm i",
    "install:app3": "cd ./app3/ && npm i",
    "start": "concurrently --kill-others --kill-others-on-fail --prefix name --names \"config,deps,header,slider,app3,utils\" \"npm run start:config\" \"npm run start:deps\" \"npm run start:header\" \"npm run start:slider\" \"npm run start:app3\" \" npm run start:utils\"",
    "start:config": "webpack-dev-server --config ./webpack.config.dev.js --port 7001",
    "start:deps": "webpack-dev-server --config ./webpack.config.deps.dev.js --port 7002",
    "start:header": "cd ./header/ && webpack-dev-server --config ./webpack.config.dev.js --port 7003",
    "start:slider": "cd ./slider/ && webpack-dev-server --config ./webpack.config.dev.js --port 7004",
    "start:app3": "cd ./app3/ && webpack-dev-server --config ./webpack.config.js --port 7005",
    "start:utils": "cd ./utils/ && webpack-dev-server --config ./webpack.config.dev.js --port 7777",
    "build": "webpack --config webpack.config.js",
    "postinstall": "node postinstall.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "single-spa": "4.0.1",
    "sofe": "2.1.4",
    "systemjs": "0.20.19",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-stage-0": "^7.0.0",
    "autoprefixer": "^9.5.0",
    "babel-loader": "8.0.0",
    "clean-webpack-plugin": "0.1.19",
    "concurrently": "4.0.1",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "1.0.0",
    "style-loader": "0.23.0",
    "webpack": "4.17.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "^3.1.14"
  }
}
